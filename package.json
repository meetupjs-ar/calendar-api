{
    "name": "calendar-api",
    "version": "1.0.1",
    "main": "index.js",
    "scripts": {
        "clean-deploys": "now rm $INSTANCE_NAME --token $NOW_TOKEN --yes",
        "deploy": "now alias $(now --name $INSTANCE_NAME -e EVENTBRITE_API=$EVENTBRITE_API -e MEETUP_API=$MEETUP_API -e NODE_ENV=production -e SPREADSHEET_API=$SPREADSHEET_API --npm --token $NOW_TOKEN) $INSTANCE_NAME --token=$NOW_TOKEN",
        "dev": "micro-dev -p ${PORT:-4000}",
        "precommit": "eslint '**/*.js'",
        "scale": "now scale $INSTANCE_NAME --token $NOW_TOKEN",
        "start": "micro"
    },
    "author": "durancristhian@gmail.com",
    "license": "MIT",
    "dependencies": {
        "color-hash": "^1.0.3",
        "dotenv": "^6.0.0",
        "got": "^8.3.1",
        "micro": "^9.3.2",
        "micro-cors": "^0.1.0",
        "moment": "^2.22.2"
    },
    "devDependencies": {
        "babel-eslint": "^8.2.3",
        "eslint": "^4.19.1",
        "husky": "^0.14.3",
        "micro-dev": "^3.0.0"
    }
}
